{
  "db": {
    "name": "db",
    "connector": "memory",
    "file": "mem_db.json"
  },
  "watson-conversation0": {
    "name": "watson-conversation0",
    "connector": "rest",
    "operations": [
      {
        "template": {
          "method": "POST",
          "url": "${CONVERSATION0_API_URL}",
          "body": "{input='':any}",
          "responsePath": "$"
        },
        "functions": {
          "message": [
            "input"
          ]
        }
      }
    ],
    "crud": false,
    "options": {
      "headers": {
        "accept": "application/json",
        "content-type": "application/json"
      }
    },
    "auth": {
      "user": "${CONVERSATION_API_USER}",
      "password": "${CONVERSATION_API_PASSWORD}"
    }
  },
  "watson-conversation1": {
    "name": "watson-conversation1",
    "connector": "rest",
    "operations": [
      {
        "template": {
          "method": "POST",
          "url": "${CONVERSATION1_API_URL}",
          "body": "{input='':any}",
          "responsePath": "$"
        },
        "functions": {
          "message": [
            "input"
          ]
        }
      }
    ],
    "crud": false,
    "options": {
      "headers": {
        "accept": "application/json",
        "content-type": "application/json"
      }
    },
    "auth": {
      "user": "${CONVERSATION_API_USER}",
      "password": "${CONVERSATION_API_PASSWORD}"
    }
  },
  "nlu": {
    "name": "nlu",
    "baseURL": "${NATURAL_LANGUAGE_URL}",
    "crud": false,
    "connector": "rest",
    "options": {
      "headers": {
        "accept": "application/json",
        "content-type": "application/json"
        },
      "auth": {
        "user": "${NATURAL_LANGUAGE_USERNAME}",
        "password": "${NATURAL_LANGUAGE_PASSWORD}"
        }
    },
    "operations": [
      {
        "template": {
          "method": "POST",
          "url": "${NATURAL_LANGUAGE_TEXT_COMBINED_URL}",
          "query": {
            "text" : "{text='':string}",
            "features" : {
              "entities": {
                "model": "c8744ad4-369a-43a9-ab84-12272f06f572"
              }
            }
          },
          "responsePath": "$"
        },
        "functions": {
          "analyzeText": [
            "text"
          ]
        }
      }
    ]
  }
}
