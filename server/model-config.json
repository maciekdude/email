{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db",
    "public": false
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "Auth": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "login": true,
          "logout": true,
          "isInRole": true
        }
      }
    }
  },
  "Conversation": {
    "dataSource": "watson-conversation",
    "public": true
  },
  "WatsonSttToken": {
    "dataSource": "watson-stt-token",
    "public": true
  },
  "Sample-Cloudant": {
    "dataSource": "cloudant",
    "public": true
  },
  "Alchemy": {
    "dataSource": "alchemy",
    "public": true
  }
}
